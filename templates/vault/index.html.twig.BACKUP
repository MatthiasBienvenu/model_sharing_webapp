{% extends 'base.html.twig' %}

{% block title %}Hello VaultController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Hello Vaults super youpi</h1>

    {% for vault in vaults %}
        <div>
            <h2>Vault {{vault.id}}</h2>

            <h3>Vault models</h3>
            <ol>
            {% for model in vault.getModels() %}
                <li>{{model.name}}</li>
            {% endfor %}
            </ol>
        </div>
    {% endfor %}
</div>
{% endblock %}
